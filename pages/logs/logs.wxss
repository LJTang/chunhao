.container{
  background:#f2f2f2;
  margin: 0;
  padding: 0;
}
.logo_Head{
  width:750rpx;
  height:200rpx;
  text-align: center;
  padding:100rpx 0;
  background:#f2f2f2;
}
.logo_Head>view{
  width:220rpx;
  height:220rpx;
  margin:0 auto;
}
.logo_Head image{
  width:220rpx;
  height:220rpx;
  border-radius:10px;
  text-align: center;
}
.view_Form{
  height:200rpx;
  width:750rpx;
  background:#fff;
  border-radius:5px;
}
.view_Form .box{
  height:200rpx;
  width:750rpx;
  background:#fff;
}
.view_Form .box .phone,.view_Form .box .password{
  width:750rpx;
  height:100rpx;
  line-height:100rpx;
  border-bottom:1rpx solid #e4e4e4;
  display:flex;
}
.view_Form .box .phone image,.view_Form .box .password image{
  width:40rpx;
  height:50rpx;
  margin-top:25rpx;
  margin-left:20rpx;
  margin-right:40rpx;
}
.view_Form .box .phone input,.view_Form .box .password input{
  width:590rpx;
  height:100rpx;
  line-height: 100rpx;
}

.layout_LR{
  height:100rpx;
  line-height:100rpx;
}
.layout_LR>view{

}
.layout_LR .layout_Right{
  margin-right: 20rpx;
}
checkbox.wx_Checkbox{
  margin-right:15rpx;
}
checkbox{
  transform: scale(0.75) translateY(0px);
  transform-origin: center right;
}
.logo_Btn{
  width:710rpx;
  height:100rpx;
  line-height: 100rpx;
  margin-top:40rpx;
  margin-left: 20rpx;
  border-radius: 5rpx;
  text-align: center;
}
.denglu{
  background:#1cb2e7;
  color:#fff;
}
.zhuce{
  border:1px solid #1cb2e7;
  color:#1cb2e7;
}
.btn{
  background:none;
  border:none;
  height:100rpx;
  color:#fff;
}