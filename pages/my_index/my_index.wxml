<!--index.wxml--><view class="container">  <image style="width:750rpx;height:350rpx;" src="/images/user_bg.png"></image>  <view class="head_Box">    <view class="display_F mal20R mar20R">      <view class="head_Img">        <view>          <image class="" src="{{userInfo.avatar==''?'/images/default.jpg':userInfo.avatar}}" mode='aspectFill'></image>        </view>        <view class="user_Identity">{{userInfo.postName}}</view>      </view>      <view class="mal40R">        <view class="f34R mat10R">{{userInfo.memberArray.name}}</view>        <view class="f30R">ID：{{userInfo.serial_number}}</view>        <view class="f30R">广州春皓商贸有限公司</view>        <view class="f28R">{{userInfo.address}}，{{userInfo.memberArray.sex==0?'男':'女'}}，{{userInfo.memberArray.age}}岁</view>      </view>    </view>    <!--<view class="lowerRight_Box">-->      <!--<view class="fR mal40R">-->        <!--<view class="f36R">11</view>-->        <!--<view class="f30R">粉丝</view>-->      <!--</view>-->      <!--<view class="fR">-->        <!--<view class="f36R">11</view>-->        <!--<view class="f30R">关注</view>-->      <!--</view>-->    <!--</view>-->  </view>  <view class="my_ListBox">    <view class="arrowType_View" bindtap="jump_Binding">      <view class="arrowType_Title"><image src="/images/bangding.png" mode='aspectFill'></image><span>绑定账号</span></view>      <view class="arrowType_Center"><input type="text" placeholder="" disabled="true"/></view>      <view class="arrowType_IMG"><image src="/images/arrow-right.png" mode='aspectFill'></image></view>    </view>    <view class="arrowType_View" bindtap="jump_DataEntry">      <view class="arrowType_Title"><image src="/images/ziliao.png" mode='aspectFill'></image><span>资料录入</span></view>      <view class="arrowType_Center"><input type="text" placeholder="" disabled="true"/></view>      <view class="arrowType_IMG"><image src="/images/arrow-right.png" mode='aspectFill'></image></view>    </view>    <view class="arrowType_View" bindtap="jump_AccountSecurity">      <view class="arrowType_Title"><image src="/images/anquan.png" mode='aspectFill'></image><span>账号安全</span></view>      <view class="arrowType_Center"><input type="text" placeholder="" disabled="true"/></view>      <view class="arrowType_IMG"><image src="/images/arrow-right.png" mode='aspectFill'></image></view>    </view>    <view class="arrowType_View" bindtap="logUot">      <view class="arrowType_Title"><image src="/images/tuichu.png" mode='aspectFill'></image><span>退出登录</span></view>      <view class="arrowType_Center"><input type="text" placeholder="" disabled="true"/></view>      <view class="arrowType_IMG"><image src="/images/arrow-right.png" mode='aspectFill'></image></view>    </view>  </view></view>